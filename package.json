{
  "name": "vs-quack",
  "displayName": "vs-quack",
  "description": "DuckDB based data processing tools for VS Code.",
  "version": "4.0.141",
  "icon": "images/logo.png",
  "publisher": "BenAidlin",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenAidlin/vs-quack"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "keywords": [
    "duckdb",
    "data",
    "sql",
    "csv",
    "parquet",
    "json",
    "vscode",
    "s3",
    "data-analysis",
    "data-processing",
    "query",
    "data-engineering",
    "notebook"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:sql",
    "onNotebook:*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-quack.runQuery",
        "title": "vs-quack: Run DuckDb Query"
      },
      {
        "command": "vs-quack.setSettings",
        "title": "vs-quack: Set DuckDb Settings"
      },
      {
        "command": "vs-quack.runTextSelectionQuery",
        "title": "vs-quack: Run Selected Text as Query"
      },
      {
        "command": "vs-quack.runQueryOnExplorer",
        "title": "vs-quack: Run Query on File"
      },
      {
        "command": "vs-quack.showQueryHistory",
        "title": "vs-quack: Show Query History"
      },
      {
        "command": "vs-quack.runCurrentVariableQuery",
        "title": "vs-quack: Evaluate Variable"
      },
      {
        "command": "vs-quack.runQueryOnFileDialog",
        "title": "vs-quack: Choose file"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vs-quack.runTextSelectionQuery",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "vs-quack.runCurrentVariableQuery",
          "when": "debugType && debugState == 'stopped'",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "vs-quack.runQueryOnExplorer",
          "when": "resourceExtname =~ /\\.(json|csv|parquet)$/i",
          "group": "navigation"
        }
      ]
    },
    "notebooks": [
      {
        "type": "vs-quack-notebook",
        "displayName": "vs-quack Notebook",
        "selector": [
          {
            "filenamePattern": "*.duckdbnb"
          }
        ]
      }
    ],
    "notebookProvider": [
      {
        "viewType": "vs-quack-notebook",
        "displayName": "VS Quack Notebook",
        "filenamePattern": "**/*.duckdbnb"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && copyfiles \"duckdb-native/**/*\" out/ && copyfiles -u 1 src/views/**/*.html out/",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@duckdb/node-api": "^1.4.1-r.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    
  }
}
